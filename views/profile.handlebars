<div class = "container content-justify-center">
  <canvas id="my-canvas"></canvas>
  <button  id="create" class="btn button-color">Create A New Post</button>
  {{#each cosplays as |cosplay|}}
    <section class="card card-style col-6 mb-1">
    <a href="/cosplay/{{id}}">
      <header>{{cosplay.name}}</header>
      <p>{{cosplay.description}}</p>
    </a>
  </section>
  {{/each}}
</div>